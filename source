Monke.MouseButton1Down:connect(function()
	local function XLIDVV_fake_script() -- ScreenGui.LocalScript 
		local script = Instance.new('LocalScript', ScreenGui)

		local set = setsimulationradius or set_simulation_radius
		game.RunService.Stepped:Connect(function()
			settings().Physics.AllowSleep = false
			game:GetService("Players").LocalPlayer.ReplicationFocus = workspace
			game:GetService("RunService").Heartbeat:Wait()
			game:GetService("Players").LocalPlayer.MaximumSimulationRadius = math.huge
			set(math.huge, math.huge)
		end)

		local plr = game.Players.LocalPlayer
		local character = plr.Character
		local bodypos = Instance.new("BodyPosition", character["HeliHat"].Handle)
		local hat = character["HeliHat"].Handle

		hat.AccessoryWeld:Destroy()

		while true do
			wait()
			bodypos.Position = character.HumanoidRootPart.CFrame:PointToWorldSpace(Vector3.new(0,4,5))
			hat.Rotation = character.HumanoidRootPart.Rotation
		end
	end
	coroutine.wrap(XLIDVV_fake_script)()
	local function MVFRKMG_fake_script() -- ScreenGui.LocalScript 
		local script = Instance.new('LocalScript', ScreenGui)

		local set = setsimulationradius or set_simulation_radius
		game.RunService.Stepped:Connect(function()
			settings().Physics.AllowSleep = false
			game:GetService("Players").LocalPlayer.ReplicationFocus = workspace
			game:GetService("RunService").Heartbeat:Wait()
			game:GetService("Players").LocalPlayer.MaximumSimulationRadius = math.huge
			set(math.huge, math.huge)
		end)

		local plr = game.Players.LocalPlayer
		local character = plr.Character
		local bodypos = Instance.new("BodyPosition", character["PlushNoob"].Handle)
		local hat = character["PlushNoob"].Handle

		hat.AccessoryWeld:Destroy()

		while true do
			wait()
			bodypos.Position = character.HumanoidRootPart.CFrame:PointToWorldSpace(Vector3.new(0,3,5))
			hat.Rotation = character.HumanoidRootPart.Rotation
		end
	end
	coroutine.wrap(MVFRKMG_fake_script)()
